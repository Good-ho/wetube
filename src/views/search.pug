extends layouts/main.pug
include mixins/videoBlock

block content
    .search__header
        h1 Searching for #{searchingBy}
    .search__videos
        if videos.length === 0
            h5 No Videos Find
        each item in videos
            +videoBlock({
                title:item.title,
                views:item.views,
                videoFile:item.fileUrl,
                id:item.id,
                creator:item.creator.id,
                avatarUrl:item.creator.avatarUrl,
                createAt:item.createAt,
                name:item.creator.name
            })
